% find how many s and c's there are, to create matrix for comparing

d = dir('./s*');
select_methods = [];
compeate_methods = [];

for f = d'
    f.name;
    parts = strsplit(f.name, '_');
    s = char(parts(1));
    s = str2double(s(2:end));
    select_methods = [select_methods s];
    c = char(parts(2));
    c = str2double(c(2:end));
    compeate_methods = [compeate_methods c];
end
compeate_methods = unique(compeate_methods)
select_methods = unique(select_methods)